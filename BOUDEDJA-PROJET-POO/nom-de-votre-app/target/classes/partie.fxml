<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url('@/JeuSnake.jpg');" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.jouerControler">
   <children>
      <ImageView fx:id="image" fitHeight="432.0" fitWidth="589.0" layoutX="6.0" layoutY="4.0" opacity="0.85" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@JeuSnake.jpg" />
         </image>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </ImageView>
      <Text fill="#d11717" layoutX="120.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Bienvenu au jeu de Snake" wrappingWidth="384.2265625">
         <font>
            <Font name="System Bold" size="26.0" />
         </font></Text>
      <Button fx:id="NouvellePartie" layoutX="246.0" layoutY="164.0" mnemonicParsing="false" onAction="#Commencer" prefHeight="24.0" prefWidth="128.0" text="Nouvelle Partie" />
      <Button fx:id="ReprendrePartie" layoutX="246.0" layoutY="214.0" mnemonicParsing="false" onAction="#Reprendre" text="Reprendre Partie" />
      <Button fx:id="quitter" layoutX="248.0" layoutY="273.0" mnemonicParsing="false" onAction="#QuitterJeu" prefHeight="24.0" prefWidth="128.0" text="Quitter" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="26.0" layoutY="188.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Slither.jpg" />
         </image>
      </ImageView>
   </children>
</Pane>
